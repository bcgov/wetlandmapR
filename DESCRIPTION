Encoding: UTF-8
LazyData: true
Depends:
    RSAGA (>= 1.3.0),
    raster,
    ModelMap (>= 3.4.0.1),
SystemRequirements: SAGA-GIS (v2.3+)
RoxygenNote: 7.1.1
